    "algoliasearch": "^5.44.0",
    "autoprefixer": "^10.4.22",
    "contentlayer2": "0.5.8",
    "date-fns": "^3.6.0",
    "dompurify": "^3.3.0",
    "framer-motion": "^11.18.2",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.554.0",
    "next": "14.2.33",
    "next-contentlayer2": "0.5.8",
    "next-image-zoom": "^1.1.7",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "pliny": "0.4.1",
    "postcss": "^8.5.6",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "18.3.1",
    "react-ga4": "^2.1.0",
    "react-icons": "^5.5.0",

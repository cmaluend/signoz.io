    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.33",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "tar": "6.2.1",
    "@sentry/react": "^7.19.1",
    "@sentry/browser": "^7.19.1",
    "form-data": "4.0.4",
    "brace-expansion": "2.0.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css)": [
      "prettier --write"
    ]
  }

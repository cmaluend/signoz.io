    "react-markdown": "^9.1.0",
    "react-medium-image-zoom": "^5.4.0",
    "react-multi-carousel": "^2.8.6",
    "reading-time": "1.5.0",
    "recharts": "^2.15.4",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-preset-minify": "7.0.1",
    "rehype-prism-plus": "^2.0.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
